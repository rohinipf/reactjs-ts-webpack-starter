    "npm-run-all": "^4.1.5",
    "postcss-loader": "^6.2.1",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.23",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "scripts": {
    "build:css": "webpack --mode=production --node-env=production",
    "build:css-dev": "webpack --mode=production --node-env=development",
    "watch:css": "webpack --mode=production --node-env=spy",
    "start": "npm-run-all --parallel watch:css start:react",
